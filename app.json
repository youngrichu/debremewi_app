{
  "expo": {
    "name": "Dubai Debremewi",
    "slug": "dubaidebremewi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash_screens/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/6985e10d-9694-4f7e-a16f-7fb1cfdc8ef1"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library so you can upload a profile picture. For example, you can select a photo from your gallery to personalize your church community profile.",
        "NSCalendarsUsageDescription": "This app needs access to your calendar to add events you're interested in.",
        "NSRemindersUsageDescription": "This app needs access to your reminders to set event reminders.",
        "LSApplicationQueriesSchemes": [
          "calshow",
          "calshow"
        ],
        "ExpoLocalization_supportsRTL": true,
        "aps": {
          "alert": {
            "badge": 1
          },
          "categories": {
            "event": {
              "actions": [
                {
                  "identifier": "view",
                  "title": "View",
                  "options": {
                    "foreground": true
                  }
                },
                {
                  "identifier": "view",
                  "title": "View",
                  "options": {
                    "foreground": true
                  }
                }
              ]
            }
          }
        },
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.debremewi.church"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.debremewi.church",
      "permissions": [
        "READ_CALENDAR",
        "WRITE_CALENDAR",
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "dubaidebremewi",
    "plugins": [
      [
        "expo-calendar",
        {
          "calendarPermission": "The app needs to access your calendar to add events."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#ffffff"
        }
      ],
      "expo-localization",
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35
          }
        }
      ]
    ],
    "extra": {
      "supportsRTL": true,
      "eas": {
        "projectId": "6985e10d-9694-4f7e-a16f-7fb1cfdc8ef1"
      }
    },
    "owner": "habtilo",
    "runtimeVersion": "1.0.0"
  }
}